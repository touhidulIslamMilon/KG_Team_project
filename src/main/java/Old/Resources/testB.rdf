<rdf:RDF
xmlns:ex="http://example.com/"
xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">

<rdf:Description rdf:about="http://example.com/person/john">
  <ex:name>John</ex:name>
  <ex:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30</ex:age>
  <ex:gender>Male</ex:gender>
</rdf:Description>

<rdf:Description rdf:about="http://example.com/person/mary">
  <ex:name>Mary</ex:name>
  <ex:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">25</ex:age>
  <ex:gender>Female</ex:gender>
</rdf:Description>

</rdf:RDF>