<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:ex="http://example.org/ontology#">

  <rdf:Description rdf:about="http://example.org/resource/Person1">
    <rdf:type rdf:resource="http://example.org/ontology#Person"/>
    <ex:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">John Doe</ex:name>
    <ex:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30</ex:age>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/resource/Person2">
    <rdf:type rdf:resource="http://example.org/ontology#Person"/>
    <ex:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Jane Smith</ex:name>
    <ex:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">25</ex:age>
  </rdf:Description>

  <rdf:Property rdf:about="http://example.org/ontology#name">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://example.org/ontology#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Property>

  <rdf:Property rdf:about="http://example.org/ontology#age">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://example.org/ontology#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </rdf:Property>

  <rdf:Class rdf:about="http://example.org/ontology#Person">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Class>

</rdf:RDF>